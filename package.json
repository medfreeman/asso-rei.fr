{
  "private": true,
  "name": "REI",
  "description": "association sportive",
  "author": "Mehdi Lahlou <mehdi.lahlou@free.fr>",
  "homepage": "https://www.asso-rei.fr/",
  "repository": "https://github.com/medfreeman/asso-rei.fr.git",
  "scripts": {
    "#<git hooks>": "handled by husky",
    "precommit": "lint-staged",
    "#</git hooks>": "handled by husky",
    "lint:js": "eslint --ignore-path .gitignore --fix \"src/**/*.js\" \"src/**/*.vue\"",
    "lint:css": "stylelint \"src/**/*.css\" \"**/*.vue\"",
    "lint": "npm-run-all --parallel lint:*",
    "test": "yarn run lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "@nuxtjs/font-awesome": "^1.0.2",
    "@nuxtjs/icon": "^2.1.0",
    "@nuxtjs/manifest": "^2.1.0",
    "@nuxtjs/meta": "^2.2.1",
    "eslint": "^5.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "git-exec-and-restage": "^1.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "netlify-cms": "^2.0.8",
    "npm-run-all": "^4.1.1",
    "nuxt": "^1.4.2",
    "nuxt-netlify-cms": "^3.1.0",
    "nuxtent": "^1.4.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-reporter": "^6.0.0",
    "prettier": "^1.7.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uuid": "^3.1.0",
    "vue-flex": "^2.0.2",
    "vue-scrollto": "^2.11.0",
    "vuetify": "^1.1.9",
    "vuex-scroll": "^0.0.5"
  },
  "lint-staged": {
    "*.js": [
      "git-exec-and-restage prettier --write --parser=babylon --"
    ],
    "*.css": [
      "git-exec-and-restage prettier --write --"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 8
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-html"
    ],
    "extends": [
      "stylelint-config-standard"
    ],
    "rules": {
      "no-empty-source": null,
      "declaration-empty-line-before": null
    }
  },
  "dependencies": {}
}
